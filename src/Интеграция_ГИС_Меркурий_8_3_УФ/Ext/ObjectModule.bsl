  		ЗапросXML = ЗапросXML + "
		|                  <dt:kpp>"+КПП+"</dt:kpp>";
	КонецЕсли;
  	ЗапросXML = ЗапросXML + "
	|                  <dt:ogrn>"+ОГРН+"</dt:ogrn>
