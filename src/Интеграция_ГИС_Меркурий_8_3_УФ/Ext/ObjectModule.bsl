Функция ПроверитьВСД2_ОбъединениеПартий(ВСДСсылка)
	Если НЕ(ЗначениеЗаполнено(ВСДСсылка)) Тогда
		Возврат 0;
	КонецЕсли;
	Если Не ЗначениеЗаполнено(ВСДСсылка.Организация) Тогда
		Сообщить("Не указана Организация в документе "+ВСДСсылка);
		Результат = 0;
	КонецЕсли;
	Если (СокрЛП(ВСДСсылка.Статус)="COMPLETED") Тогда
		Сообщить(""+ВСДСсылка+" статус "+ВСДСсылка.Статус+" НЕ отправляем");
		Возврат 0;
	КонецЕсли;
	//Проверить Отправителя ХС
//	Если НЕ(ВСДСсылка.Владелец_ХозСубъект = Отправитель_Хозсубъект) Тогда
//		Сообщить("Другой ХС Производитель - Выберите в Параметрах организацию для  "+ВСДСсылка.Владелец_ХозСубъект);
//		Возврат 0;
//	КонецЕсли;
	Если Не ВСДСсылка.ОперацияПрисоединения Тогда
		Если НЕ ЗначениеЗаполнено(ВСДСсылка.Продукция_Элемент) Тогда
			Сообщить("При объединении Партий значение Продукция_Элемент Обязательно для Заполнения !!!");
			Возврат 0;
		КонецЕсли;
	КонецЕсли;
	ЕстьОшибки = false;
	Для каждого стрПродукция из ВСДСсылка.Продукция Цикл
		Если НЕ(ЗначениеЗаполнено(стрПродукция.Партия)) Тогда
			Сообщить("Не указана Партия в документе "+ВСДСсылка+" не отправлен!");
			ЕстьОшибки = true;
			Прервать;
		КонецЕсли;
		Если НЕ(ЗначениеЗаполнено(стрПродукция.ДатаИзготовления1)) или НЕ(ЗначениеЗаполнено(стрПродукция.ДатаСрокГодности1)) Тогда
			ЕстьОшибки = true;
			Сообщить("Не указаны Дата Изготовления и/или Срок годности  "+стрПродукция.Партия+" в документе "+ВСДСсылка+" не отправлен!");
			Прервать;
		КонецЕсли;
		
	КонецЦикла;
	Если ЕстьОшибки Тогда
		Возврат 0;
	Иначе
		Возврат 1;
	КонецЕсли;
КонецФункции

